{
    "manifest_version": 3,
    "version": "0.1",
    "name": "My Note App",
    "background": {
        "service_worker": "background.js"
      },
    "icons": { "16": "icons/notepad-icon16.png",
        "48": "icons/notepad-icon48.png",
        "128": "icons/notepad-icon128.png" },
    "permissions": ["storage", "tabs"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icons/notepad-icon48.png"
    }
}